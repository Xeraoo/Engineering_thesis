/***** End of imports. If edited, may not auto-convert in the playground. *****/
//Tymoteusz Maj
//Informatyka Geoprzestrzenna (Geoinformacja)
//Praca inżynierska

// Ustawienie obszaru roboczego
Map.setCenter(18.8, 50.3, 10); // Wyśrodkuj mapę na obszarze roboczym

// Define the geographical area of interest using a rectangle
var GZMRectangle = ee.Geometry.Rectangle([18.25, 49.95, 19.65, 50.55]);

// Adding the Boundaries_GZM asset extracted with QGIS from the counties of Poland to the work.
var GZMBoundaries = ee.FeatureCollection("users/tymoteuszmaj/GZM");

// Dodaj warstwę GZMBoundaries jako delikatny zarys
Map.addLayer(
  GZMBoundaries,
  {color: '000000', width: 1, opacity: 0.5},
  'GZM Boundaries'
);

// Determining the year for analysis
var analysisYear = 2006;
var startDate = ee.Date.fromYMD(analysisYear, 5, 1);
var endDate = ee.Date.fromYMD(analysisYear, 10, 1);

// Load Landsat 8 Collection 2 Level-2 Tier-1 data
var landsatCollection = ee.ImageCollection("LANDSAT/LT05/C02/T1_L2")
  .filterBounds(GZMRectangle)
  .filterDate(startDate, endDate)
  .sort("CLOUD_COVER", true)
  .limit(20);

// Select the median image from the collection
var medianImage = landsatCollection.median();

// Select surface temperature band
var ST_B10 = medianImage.select(['ST_B6']);

// Function to convert scaled Kelvin data to temperature in Celsius
var convertToTemperature = function(image) {
  var temperatureKelvin = image.multiply(0.00341802);
  var temperatureCelsiusRaw = temperatureKelvin.subtract(273.15);
  var temperatureCelsius = temperatureCelsiusRaw.add(149);
  return temperatureCelsius;
};

// Convert surface temperature to Celsius
var temperature = convertToTemperature(ST_B10).clip(GZMBoundaries);

// Calculate z-scores for temperature
var meanStd = temperature.reduceRegion({
  reducer: ee.Reducer.mean().combine(ee.Reducer.stdDev(), null, true),
  geometry: GZMRectangle,
  scale: 30,
  maxPixels: 1e9
});

var meanTemperature = ee.Number(meanStd.get('ST_B6_mean'));
var stdDevTemperature = ee.Number(meanStd.get('ST_B6_stdDev'));

var zScores = temperature.subtract(meanTemperature).divide(stdDevTemperature);

// Set threshold for detecting outliers (e.g., z-score greater than 2)
var zScoreThreshold = 2;

// Create a mask for outliers
var outlierMask = zScores.abs().gt(zScoreThreshold);

// Apply the outlier mask to the temperature data
var temperatureOutliers = temperature.updateMask(outlierMask);

// Define a more varied color palette for temperatures from +30°C to +60°C with color intervals of 5°C
var TemperaturePalette = [
  'FFFF00', 'FFD700', 'FFA500', 'FF6347', 'FF4500', 'FF0000', '8B0000', '800000', '990000', 'A52A2A', 'B22222'
];

// Add the land surface temperature layer to the map
Map.addLayer(
  temperatureOutliers,
  {min: 30, max: 60, palette: TemperaturePalette},
  'Temperature (°C)'
);

// Add a legend to the map
var legend = ui.Panel({
  style: {
    position: 'bottom-right',
    padding: '8px 15px'
  }
});

var legendTitle = ui.Label({
  value: 'Temperature (°C)',
  style: {
    fontWeight: 'bold',
    fontSize: '18px',
    margin: '0 0 4px 0',
    padding: '0'
  }
});

legend.add(legendTitle);

var makeRow = function(color, label) {
  var colorBox = ui.Label({
    style: {
      backgroundColor: color,
      padding: '8px',
      margin: '0 0 4px 0'
    }
  });

  var description = ui.Label({
    value: label,
    style: {
      margin: '0 0 4px 6px'
    }
  });

  return ui.Panel({
    widgets: [colorBox, description],
    layout: ui.Panel.Layout.Flow('horizontal')
  });
};

legend.add(makeRow('FFFF00', '30°C'));
legend.add(makeRow('FFD700', '35°C'));
legend.add(makeRow('FFA500', '40°C'));
legend.add(makeRow('FF6347', '45°C'));
legend.add(makeRow('FF4500', '50°C'));
legend.add(makeRow('FF0000', '55°C'));

Map.add(legend);